<view class="section-user">
  <open-data type="userAvatarUrl"></open-data>
  <open-data type="userNickName"></open-data>
</view>

<view class="line-m"></view>

<view class="section-module">
  <view class="section-item" bindtap='doPhone'>
    <image src="/images/coordinate.png"></image>
    <view>预约看房</view>
    <span></span>
  </view>
</view>
<view class="section-module">
  <view class="section-item item-contact">
    <image src="/images/contact.png"></image>
    <view>联系客服</view>
    <span></span>
    <button open-type="contact" plain='{{true}}'></button>
  </view>
</view>

<view class="line-m min"></view>

